{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
  ],
  "stylelint.validate": [
    "css",
    "less",
    "scss",
    "postcss"
  ],
  "editor.inlineSuggest.showToolbar": "always",
}