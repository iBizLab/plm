    "getPSDEFormItemUpdates" : [ {
      "codeName" : "duration_updateform",
      "memo" : "var form_data = view.layoutPanel.panelItems.form.control.data;\nvar remaining_workload = form_data.remaining_workload;\nvar actual_workload = form_data.actual_workload;\nvar estimated_workload = form_data.estimated_workload;\nvar duration_workload = form_data.duration;\nvar duration = 0;\nif(duration_workload){\n\tduration = Number(duration_workload);\n}\n// 已登记的实际工时\nvar actual = 0; \nif(actual_workload){\n\tactual = Number(actual_workload);\n}\nvar estimated = 0; // 预估工时\nvar remaining = 0; // 预估工时\nif(estimated_workload){\n\testimated = Number(estimated_workload);\n\t// 重置剩余工时\n\tremaining = estimated - duration - actual;\n\tif(remaining <= 0){\n\t\tremaining = 0;\n\t}\n\tform_data.remaining_workload = remaining;\n}\n \n// 重置工时进度\nif((actual + duration + remaining) != 0){\n\tvar schedule = (((actual + duration) / (actual + remaining + duration)) * 100).toFixed(1);\n\tform_data.workload_schedule = schedule;\n}",
      "name" : "时长表单项更新",
      "getPSDEFIUpdateDetails" : [ {
        "name" : "remaining_workload"
      } ],
      "scriptCode" : "var form_data = view.layoutPanel.panelItems.form.control.data;\r\nvar remaining_workload = form_data.remaining_workload;\r\nvar actual_workload = form_data.actual_workload;\r\nvar estimated_workload = form_data.estimated_workload;\r\nvar duration_workload = form_data.duration;\r\nvar duration = 0;\r\nvar actual = 0;\r\nvar estimated = 0;\r\nvar remaining = 0; // 剩余工时\r\nif(duration_workload){\r\n\tduration = Number(duration_workload);\r\n}\r\n// 已登记的预估工时\r\nif(estimated_workload){\r\n\testimated = Number(estimated_workload);\r\n}\r\n// 已登记的实际工时\r\nif(actual_workload){\r\n\tactual = Number(actual_workload);\r\n\t// 重置剩余工时\r\n\tremaining = estimated + duration - actual;\r\n\tif(remaining <= 0){\r\n\t\tremaining = 0;\r\n\t}\r\n} else {\r\n\tremaining = estimated + duration;\r\n}\r\nreturn {remaining_workload:remaining};",
      "customCode" : true
    } ],
