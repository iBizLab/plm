    "getPSDEFormItemUpdates" : [ {
      "codeName" : "duration_updateform",
      "name" : "时长表单项更新",
      "getPSDEFIUpdateDetails" : [ {
        "name" : "remaining_workload"
      } ],
      "scriptCode" : "const _form = view.getController('form');\r\nconst _form_set = _form.details;\r\nvar form_data = _form.state.data;\r\nvar remaining_workload = form_data.remaining_workload;\r\nvar actual_workload = form_data.actual_workload;\r\nvar estimated_workload = form_data.estimated_workload;\r\nvar duration_workload = form_data.duration;\r\nvar duration = 0;\r\nvar actual = 0;\r\nvar estimated = 0;\r\nvar remaining = 0; // 剩余工时\r\nif(duration_workload){\r\n\tduration = Number(duration_workload);\r\n}\r\n// 已登记的预估工时\r\nif(estimated_workload){\r\n\testimated = Number(estimated_workload);\r\n}\r\n// 已登记的实际工时\r\nif(actual_workload){\r\n\tactual = Number(actual_workload);\r\n\t// 重置剩余工时\r\n\tremaining = estimated + duration - actual;\r\n\tif(remaining <= 0){\r\n\t\tremaining = 0;\r\n\t}\r\n} else {\r\n\tremaining = estimated + duration;\r\n}\r\n_form_set.remaining_workload.setDataValue(remaining);",
      "customCode" : true
    } ],
