                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="article_pagespace_shared_searchprepareparam2" id="prepareparam2" implementation="http://www.jboss.org/drools/rule" name="页面标识需存在共享空间的shared_pages属性中"/>
