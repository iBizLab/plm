                                eval((RuleUtils.test($article_pagespace_shared_searchshared_space.get("isShared"),"EQ","2")))
                                ]]>
                            </conditionExpression>
                    </sequenceFlow>
                    <sequenceFlow id="gateway-deaction1_dedataset1" sourceRef="gateway-deaction1" targetRef="dedataset1">
